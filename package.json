{
  "name": "@aragornhq/ch-migration",
  "version": "1.0.0",
  "description": "Production-grade ClickHouse migration CLI with rollback, test coverage, and GitHub Actions support",
  "repository": {
    "type": "git",
    "url": "https://github.com/aragornhq/ch-migration.git"
  },
  "homepage": "https://github.com/aragornhq/ch-migration#readme",
  "bugs": {
    "url": "https://github.com/aragornhq/ch-migration/issues"
  },
  "main": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "bin": {
    "ch-migrate": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "migration:create": "ts-node src/cli.ts migration:create",
    "migration:up": "ts-node src/cli.ts migration:up",
    "migration:down": "ts-node src/cli.ts migration:down",
    "test": "jest"
  },
  "keywords": [
    "clickhouse",
    "migration",
    "typescript",
    "cli",
    "rollback"
  ],
  "author": "Aragorn AI",
  "license": "MIT",
  "dependencies": {
    "@clickhouse/client": "^1.11.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
